%h1 Editing User
= form_for user, url: edit_admin_user_path(user) do |f|
	- if user.errors.any?
		#error_explanation
			%h2= "#{pluralize(user.errors.count, "error")} prohibited this admin_user from being saved:"
			%ul
				- user.errors.full_messages.each do |msg|
					%li= msg
	= render 'form', locals: {f: f}

= link_to 'Show', user
\|
= link_to 'Back', admin_users_path
