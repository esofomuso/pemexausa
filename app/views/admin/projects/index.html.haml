.admin_panel_header
	%h1
		Listing projects
.admin_panel_inner
	%table{id: 'admin_projects', style: "width: 100%;"}
		%thead
			%tr
				%th
				%th
				%th
				
		%tbody
			- projects.each do |admin_project|
				%tr
					%td= link_to 'Show', admin_project
					%td= link_to 'Edit', edit_admin_project_path(admin_project)
					%td= link_to 'Destroy', admin_project, :method => :delete, :data => { :confirm => 'Are you sure?' }
	
	%br
	
	= link_to 'New Project', new_admin_project_path
