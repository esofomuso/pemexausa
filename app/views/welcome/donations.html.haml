			ErrString = ErrString + "Donor's Name is Required.";
			IsSuccessful = false;
		}

		if ($.trim($("#Email").val()).length == 0) {
			ErrString = ErrString + "Donor's Email is Required.";
			IsSuccessful = false;
		}
		else if (ValidateEmailAddress($.trim($("#Email").val())) == false) {
			ErrString = ErrString + "Donor's Email is Invalid.";
			IsSuccessful = false;
		}

		if ($.trim($("#refer").val()).length == 0) {
			ErrString = ErrString + "Referrer is Required.";
			IsSuccessful = false;
		}

		if ($.trim($("#referEmail").val()).length == 0) {
			ErrString = ErrString + "Referrer's Email is Required.";
			IsSuccessful = false;
		}
		else if (ValidateEmailAddress($.trim($("#referEmail").val())) == false) {
			ErrString = ErrString + "Donor's Email is Invalid.";
			IsSuccessful = false;
		}

		return IsSuccessful;
	}
	
